{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "lib": ["DOM", "ES2019"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "lib",
    "rootDir": "src",
    "sourceMap": false,
    "strict": true,
    "target": "ES2019",
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"]
}
